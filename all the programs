print("result:",largest)


#check the age if you child,teenage,adult
age=int(input("enter the your age:"))
if(age<13):
    print("child")
if(age<18):
    print("teenager")
else:
    print("adult")



    #check the grades using marks
    marks=int(input("enter the marks"))
if(marks>90):
    print("Grade A")
elif(marks>70 & marks<80):
    print("Grade B")
elif(marks>50 & marks<70):
    print("Grade C")          
elif(marks>50 & marks<40):
    print("Grade D")    
else:
    print("faiL")
