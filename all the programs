# swap 2no's without 3rd varaiable
a=int(input("enter the num"))
b=int(input("enter the num"))
a=a+b
b=a-b
a=a-b
print("a:",a)
print("b:",b)


#swap 2no's using 3rd varabile
a=int(input("enter the num"))
b=int(input("enter the num"))
temp=a
a=b
b=temp
print("after swapping:",a)
print("after swapping:",b)



