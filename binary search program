def binary_search(arr,target):
    ib=0
    ub=len(arr)-1
    while ib<=ub:
        mid=(ib+ub)
        if arr[mid]==target:
            return mid
        elif arr[mid]<target:
            ib=mid+1
        else:
            ub=mid-1
    return-1
arr=[1,3,5,7,9,11]
target=7
result=binary_search(arr,target)
if result!=-1:
    print(f"element found at index {result}")
else:
    print("element not found")    
